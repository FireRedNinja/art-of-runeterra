---
import ImageGrid from "../components/ImageGrid";
import Layout from "../layout/layout.astro";
import { data, globals } from "../data/";

const gridData = globals.sets.map((set) => ({
	imgPath: set.iconAbsolutePath,
	title: set.name,
	key: set.nameRef,
	url: `/set/${set.nameRef}`
}));
---

<Layout>
  <ImageGrid data={gridData} />
</Layout>
