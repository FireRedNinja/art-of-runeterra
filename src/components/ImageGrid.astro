---
import ImageTile from "./ImageTile.astro";

const { data } = Astro.props;
---

<div class="grid sm:grid-cols-2 md:grid-cols-3 lg:grid-cols-4 2xl:grid-cols-5">
  {
    data.map((item) => (
      <div>
        <ImageTile title={item.title} imgPath={item.imgPath} url={item.url} />
      </div>
    ))
  }
</div>
